<h1>Create a New User</h1>

<%= form_for @user, url: users_path do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_area :email %>
  </div>

  <div>
    <%= f.label :age %>
    <%= f.text_area :age %>
  </div>

  <div>
  <%= f.label :city %>
  <%= f.text_area :city %>
</div>

  <div>
    <%= f.submit "Create User" %>
  </div>
<% end %>
