<h1>Edit the Room Post</h1>

<%= form_for @room_post do |f| %>
  <div>
    <%= f.label :owner %>
    <%= f.text_field :owner %>
  </div>

  <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit "Guardar Cambios" %>
  </div>
<% end %>
