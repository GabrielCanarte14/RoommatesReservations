<h1>List of Users</h1>
<ul>
  <% @users.each do |user| %>
    <li><strong>Name:</strong> <%= user.name %>, <strong>Email:</strong> <%= user.email %></li>
    <%= link_to "Enviar Mensaje", new_message_path(to_user: user.username, from_user: "NombreDeRemitente") %>
  <% end %>
</ul>
<%= link_to "Favoritos", favorites_path %>
<%= form_tag users_by_city_path, method: :get do %>
  <%= label_tag :city, 'Filter by City:' %>
  <%= text_field_tag :city %>
  <%= submit_tag 'Filter' %>
<% end %>